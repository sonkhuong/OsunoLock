<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/managerScanLockFragment">
    <fragment
        android:id="@+id/managerScanLockFragment"
        android:name="com.dagoras.osunolock.scanlock.fragment.ManagerScanLockFragment"
        android:label="ManagerScanLockFragment" >
        <action
            android:id="@+id/action_managerScanLockFragment_to_fragmentScanLockWithBLT2"
            app:destination="@id/scanLockWithBLT2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/scanLockWithBLT2Fragment"
        android:name="com.dagoras.osunolock.scanlock.fragment.ScanLockWithBLTFragment"
        android:label="ScanLockWithBLTFragment" >
        <action
            android:id="@+id/action_fragmentScanLockWithBLT2_to_managerLockFragment"
            app:destination="@id/managerLockFragment" />
    </fragment>
    <fragment
        android:id="@+id/managerLockFragment"
        android:name="com.dagoras.osunolock.managerlock.fragment.ManagerLockFragment"
        android:label="ManagerLockFragment" >
        <action
            android:id="@+id/action_managerLockFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_settingsFragment_to_remoteUnlockFragment"
            app:destination="@id/remoteUnlockFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_lockInformationFragment"
            app:destination="@id/lockInformationFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_autoLockFragment"
            app:destination="@id/autoLockFragment" />
    </fragment>
    <fragment
        android:id="@+id/remoteUnlockFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.remote_unlock.RemoteUnlockFragment"
        android:label="RemoteUnlockFragment" />
    <fragment
        android:id="@+id/lockInformationFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.information.LockInformationFragment"
        android:label="LockInformationFragment" />
    <fragment
        android:id="@+id/autoLockFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.autolock.AutoLockFragment"
        android:label="AutoLockFragment" />
</navigation>