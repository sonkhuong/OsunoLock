<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/managerScanLockFragment">
    <fragment
        android:id="@+id/managerScanLockFragment"
        android:name="com.dagoras.osunolock.scanlock.fragment.ManagerScanLockFragment"
        android:label="ManagerScanLockFragment">
        <action
            android:id="@+id/action_managerScanLockFragment_to_fragmentScanLockWithBLT2"
            app:destination="@id/scanLockWithBLT2Fragment" />
        <action
            android:id="@+id/action_managerScanLockFragment_to_systemSettingFragment2"
            app:destination="@id/systemSettingFragment" />
    </fragment>
    <fragment
        android:id="@+id/scanLockWithBLT2Fragment"
        android:name="com.dagoras.osunolock.scanlock.fragment.ScanLockWithBLTFragment"
        android:label="ScanLockWithBLTFragment">
        <action
            android:id="@+id/action_fragmentScanLockWithBLT2_to_managerLockFragment"
            app:destination="@id/managerLockFragment" />
    </fragment>
    <fragment
        android:id="@+id/managerLockFragment"
        android:name="com.dagoras.osunolock.managerlock.fragment.ManagerLockFragment"
        android:label="ManagerLockFragment">
        <action
            android:id="@+id/action_managerLockFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.dagoras.osunolock.submanagerlock.settings.fragment.SettingsFragment"
        android:label="SettingsFragment">
        <action
            android:id="@+id/action_settingsFragment_to_remoteUnlockFragment"
            app:destination="@id/remoteUnlockFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_lockInformationFragment"
            app:destination="@id/lockInformationFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_autoLockFragment"
            app:destination="@id/autoLockFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_diagnosisFragment"
            app:destination="@id/diagnosisFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_lockSoundFragment"
            app:destination="@id/lockSoundFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_resetButtonFragment"
            app:destination="@id/resetButtonFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_uploadDataFragment"
            app:destination="@id/uploadDataFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_tamperAlertFragment"
            app:destination="@id/tamperAlertFragment" />
    </fragment>
    <fragment
        android:id="@+id/remoteUnlockFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.remote_unlock.RemoteUnlockFragment"
        android:label="RemoteUnlockFragment" />
    <fragment
        android:id="@+id/systemSettingFragment"
        android:name="com.dagoras.osunolock.menu.SystemSettingFragment"
        android:label="SystemSettingFragment" />
    <fragment
        android:id="@+id/lockInformationFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.lock_information.LockInformationFragment"
        android:label="LockInformationFragment" />
    <fragment
        android:id="@+id/autoLockFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.auto_lock.AutoLockFragment"
        android:label="AutoLockFragment" />
    <fragment
        android:id="@+id/diagnosisFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.diagnosis.DiagnosisFragment"
        android:label="DiagnosisFragment" />
    <fragment
        android:id="@+id/lockSoundFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.lock_sound.LockSoundFragment"
        android:label="LockSoundFragment" />
    <fragment
        android:id="@+id/resetButtonFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.reset_button.ResetButtonFragment"
        android:label="ResetButtonFragment" />
    <fragment
        android:id="@+id/uploadDataFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.upload_data.UploadDataFragment"
        android:label="UploadDataFragment" />
    <fragment
        android:id="@+id/tamperAlertFragment"
        android:name="com.dagoras.osunolock.submanagerlock.fragment.settings.tamper_alert.TamperAlertFragment"
        android:label="TamperAlertFragment" />
</navigation>